


struct Point {
	float x,
	float y,
};



function add(Point a, Point b) Point {
	Point result;
	result.x = a.x + b.x;
	result.y = a.y + b.y;
	return result;
}

function mul(Point a, Point b) Point {
	Point result;
	result.x = a.x * b.x;
	result.y = a.y * b.y;
	return result;
}

function mul(Point a, any b) Point {
	Point result;
	result.x = a.x * b;
	result.y = a.y * b;
	return result;
}



Point p = Point{0, 0};
